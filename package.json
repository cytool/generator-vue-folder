{
	"name": "generator-vue-folder",
	"displayName": "generator-vue-folder",
	"description": "快速生成vue页面目录",
	"version": "0.0.1",
	"publisher": "wulala",
	"repository": "https://github.com/cytool/generator-vue-folder",
	"engines": {
		"vscode": "^1.53.0"
	},
	"categories": [
		"Other"
	],
	"activationEvents": [
		"onCommand:generator-vue-folder.vue"
	],
	"main": "./extension.js",
	"contributes": {
		"commands": [
			{
				"command": "generator-vue-folder.vue",
				"title": "CYTOOL - 快速生成VUE页面目录"
			}
		],
		"menus": {
			"explorer/context": [
				{
					"command": "generator-vue-folder.vue",
					"when": "explorerResourceIsFolder && resourcePath =~ /.\/src/"
				}
			]
		},
		"snippets": [
			{
				"language": "vue",
				"path": "./tpl/vue.json"
			}
		]
	},
	"scripts": {
		"lint": "eslint .",
		"pretest": "yarn run lint",
		"test": "node ./test/runTest.js"
	},
	"devDependencies": {
		"@types/vscode": "^1.53.0",
		"@types/glob": "^7.1.3",
		"@types/mocha": "^8.0.4",
		"@types/node": "^12.11.7",
		"eslint": "^7.19.0",
		"glob": "^7.1.6",
		"mocha": "^8.2.1",
		"typescript": "^4.1.3",
		"vscode-test": "^1.5.0"
	}
}